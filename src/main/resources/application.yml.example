web-monitor:
  configs:
    - type: ZzFetcher
      name: 1111
      url:
      intervalSeconds: 600
      enabled: false

    - type: CssSelectorFetcher
      name: ssss
      url: https://www.baidu.com
      intervalSeconds: 600
      enabled: false
      cssSelector: "*[id=\"lg\"] > map > area[shape='rect']"

    - type: XPathFetcher
      name: ssss
      url: https://www.baidu.com
      intervalSeconds: 600
      enabled: false
      xPath: "*[id=\"lg\"] > map > area[shape='rect']"

  observers:
    # 控制台log
    - type: ConsoleObserver
      enabled: true

    # 邮件配置（QQ邮箱）
    - type: EmailObserver
      enabled: false
      host: smtp.example.com
      port: 587
      username: your-email@example.com
      password: your-email-password
      from: your-email@example.com
      to: recipient@example.com

    # Slack机器人配置
    - type: SlackObserver
      enabled: false
      webhook:
        base-url:

    # 企业微信机器人配置
    - type: QyWeixinObserver
      enabled: false
      webhook:
        base-url:
        key:


